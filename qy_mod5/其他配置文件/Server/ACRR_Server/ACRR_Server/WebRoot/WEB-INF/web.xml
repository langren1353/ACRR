	<servlet-mapping>
		<servlet-name>upload</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>getOrders</servlet-name>
		<url-pattern>/getOrders</url-pattern>
	</servlet-mapping>
	<servlet-mapping>

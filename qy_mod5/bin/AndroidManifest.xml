<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="2" android:versionName="1.0" package="com.xxxxx.ac">

    <!-- 版本更新需要修改versionCode -->
    <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="21"/>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS"/>
    <uses-permission android:name="android.permission.ACCESS_MOCK_LOCATION"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>

    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:name="com.xxxxx.ac.Activity.MyApplication" android:theme="@style/AppTheme">

        <!-- 定位需要的服务，定位需要加上这个 -->
        <service android:name="com.amap.api.location.APSService">
        </service>

        <meta-data android:name="com.amap.api.v2.apikey" android:value="2595df0c32855ecaa88567227cb7f06f"/>
        <meta-data android:name="5709f7e367e58e39e400151f" android:value="YOUR_APP_KEY">
        </meta-data>
        <meta-data android:name="QYMod" android:value="Channel ID"/>

        <activity android:label="@string/app_name" android:name="com.xxxxx.ac.Activity.MainActivity" android:theme="@android:style/Theme.Light.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/news_info" android:name="com.xxxxx.ac.Activity.NewsDetailActivity" android:theme="@android:style/Theme.Light.NoTitleBar"/>
        <activity android:label="@string/user_info" android:name="com.xxxxx.ac.Activity.UserInfoActivity" android:theme="@android:style/Theme.Light.NoTitleBar"/>
        <activity android:label="@string/title_activity_shop_info" android:name="com.xxxxx.ac.Activity.ShopInfoActivity" android:theme="@android:style/Theme.Light.NoTitleBar">
        </activity>
        <activity android:label="@string/title_activity_gdmap" android:name="com.xxxxx.ac.Activity.GDMapActivity" android:theme="@android:style/Theme.Light.NoTitleBar">
        </activity>
        <activity android:label="@string/title_activity_register" android:name="com.xxxxx.ac.Activity.UserRegisterActivity" android:theme="@android:style/Theme.Light.NoTitleBar">
        </activity>
        <activity android:label="@string/title_activity_user_login" android:name="com.xxxxx.ac.Activity.UserLoginActivity" android:theme="@android:style/Theme.Light.NoTitleBar">
        </activity>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.alibaba.sdk.android.feedback.windvane.CustomHybirdActivity" android:screenOrientation="portrait" android:theme="@style/ali_feedback_contentoverlay"/>
        <activity android:name="com.alibaba.sdk.android.webview.BaseWebViewActivity"/>
        <activity android:name="com.alibaba.sdk.android.webview.feedback.FeedbackActivity" android:theme="@style/feedback_title">
        </activity>
        <activity android:name="com.alibaba.sdk.android.webview.feedback.SendFeedbackActivity" android:theme="@style/feedback_title">
        </activity>

        <receiver android:exported="false" android:name="com.alibaba.sdk.android.hotpatch.HotPatchBroadcastReceiver" android:process=":hotpatch">
            <intent-filter>
                <action android:name="com.alibaba.sdk.android.hotpatch.action.HOT_PATCH"/>
            </intent-filter>
        </receiver>
    </application>
    <!-- <uses-permission android:name="android.permission.READ_PHONE_STATE" /> -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <uses-permission android:name="android.permission.READ_SETTINGS"/>
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>
    <uses-permission android:name="android.permission.GET_TASKS"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>

</manifest>
